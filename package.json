{
  "name": "adventure-time",
  "version": "1.0.0",
  "description": "a web UI for doing nodeschool adventures in the browser",
  "main": "index.js",
  "scripts": {
    "start": "beefy demo.js:demo-bundle.js terminal.js:terminal-bundle.js",
    "build": "browserify demo.js -o demo-bundle.js && browserify terminal.js -o terminal-bundle.js",
    "deploy": "gh-pages-deploy"
  },
  "author": "max ogden",
  "license": "BSD",
  "dependencies": {
    "defaultcss": "^1.1.0",
    "docker-browser-console": "^4.0.0",
    "edit": "^1.0.0",
    "flatui": "^1.0.0",
    "gh-pages-deploy": "^0.1.1",
    "iframe": "^0.3.0",
    "insert-css": "^0.2.0",
    "term.js": "0.0.4",
    "tree-view": "^2.2.0",
    "websocket-stream": "^1.0.2",
    "xhr": "^1.17.0"
  },
  "gh-pages-deploy": {
    "prep": [
      "build"
    ],
    "noprompt": "false"
  },
  "devDependencies": {
    "beefy": "^2.1.1",
    "browserify": "^6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/maxogden/adventure-time.git"
  },
  "bugs": {
    "url": "https://github.com/maxogden/adventure-time/issues"
  },
  "homepage": "https://github.com/maxogden/adventure-time"
}
